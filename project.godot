; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "CanvasLayer",
"class": "GUI",
"language": "GDScript",
"path": "res://src/gui/GUI.gd"
}, {
"base": "Node",
"class": "Inventory",
"language": "GDScript",
"path": "res://src/scenes/Inventory.gd"
}, {
"base": "StaticBody2D",
"class": "Repository",
"language": "GDScript",
"path": "res://src/environment/tree/Repository.gd"
}, {
"base": "Area2D",
"class": "Ressource",
"language": "GDScript",
"path": "res://src/ressources/Ressource.gd"
}, {
"base": "Area2D",
"class": "Tool",
"language": "GDScript",
"path": "res://src/tools/Tool.gd"
} ]
_global_script_class_icons={
"GUI": "",
"Inventory": "",
"Repository": "",
"Ressource": "",
"Tool": ""
}

[application]

config/name="Goatman"
run/main_scene="res://src/scenes/main/Main.tscn"
config/icon="res://icon.png"

[display]

window/size/width=240
window/size/height=160
window/size/test_width=960
window/size/test_height=640
window/stretch/mode="2d"
window/stretch/aspect="keep"

[layer_names]

2d_physics/layer_1="Hero"
2d_physics/layer_2="Enemies"
2d_physics/layer_3="Interactables"
2d_physics/layer_4="Obstacles"
2d_physics/layer_5="Pickups"
2d_physics/layer_6="Tools"

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/2d/use_pixel_snap=true
environment/default_environment="res://default_env.tres"
